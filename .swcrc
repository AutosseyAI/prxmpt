{
  "$schema": "https://json.schemastore.org/swcrc.json",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "target": "es2019",
    "transform": {
      "react": {
        "pragma": "Prxmpt.createElement",
        "pragmaFrag": "Prxmpt.Fragment",
        "throwIfNamespace": true,
        "development": false,
        "useBuiltins": false
      }
    }
  },
  "module": {
    "type": "es6"
  }
}
