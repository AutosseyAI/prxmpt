{
  "name": "prxmpt",
  "version": "0.1.0",
  "description": "Prompt with JSX",
  "license": "MIT",
  "private": true,
  "author": "Connor White",
  "type": "module",
  "main": ".auto/build/index.js",
  "types": ".auto/build/index.d.ts",
  "files": [
    ".auto/build/**/!(tsconfig.tsbuildinfo)"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "auto build",
    "lint": "auto lint",
