    "test": "auto test",
    "test-d": "auto test types",
    "typecheck": "auto typecheck"
  },
  "dependencies": {
    "as-typed-array": "^3.0.1",
    "types-json": "^4.0.1",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@autosoft/tsconfig": "^1.0.1",
    "tsx": "^3.12.7"
  },
  "eslintConfig": {
    "extends": "@autosoft/eslint-config"
  },
  "jest": {
    "preset": "@autosoft/jest-preset"
  },
  "npmpackagejsonlint": {
    "extends": "npm-package-json-lint-config-auto"
  }
}
